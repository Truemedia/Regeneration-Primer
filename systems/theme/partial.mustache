<!-- Theme -->
<div class="modal-header">
	<h3 id="theme-system-name">Themes</h3>
</div>
<div class="modal-body" style="max-height: 600px;">
	<section id="tables">
  		<table id="swatch-menu" class="table table-bordered table-striped table-hover" style="margin-bottom: 10px;">
    		<thead>
      			<tr>
        			<th>Theme</th>
        			<th>Version</th>
        			<th>Description</th>
        			<th>Actions</th>
      			</tr>
    		</thead>
    		<tbody>{{#themes}}
     			<tr>
     				<td>{{name}}</td>
     				<td>{{version}}</td>
     				<td>{{description}}</td>
     				<td><a class="bootswatch-theme" href="{{cssMin}}" onclick="return false;"><img src="{{thumbnail}}" alt="{{description}}" style="display: none;" />Apply</a></td>
     			</tr>
    		{{/themes}}</tbody>
  		</table>
	</section>
	<div class="modal-footer">
		<button class="btn" data-dismiss="modal" aria-hidden="true">Close</button>
	</div>
</div>