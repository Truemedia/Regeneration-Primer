<!-- Main menu -->
<div class="modal-dialog">
	<div class="modal-content">
		<div class="modal-header">
			<button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
			<h4 id="audio-system-name">{{trans.audio_noun}}</h4>
		</div>
		<div class="modal-body">
			<ul id="mixer_channels" class="well">
				{{#items}}
				<li>
					<div class="level_channel"></div>
					<div class="controls_channel">
						<!--<input class="rotary_encoder high_high_pot" type="text" value="100" />
						<input class="rotary_encoder low_high_pot" type="text" value="100" />
						<input class="rotary_encoder high_high_mid_pot" type="text" value="100" />
						<input class="rotary_encoder low_high_mid_pot" type="text" value="100" />
						<input class="rotary_encoder high_low_mid_pot" type="text" value="100" />
						<input class="rotary_encoder low_low_mid_pot" type="text" value="100" />
						<input class="rotary_encoder high_low_pot" type="text" value="100" />
						<input class="rotary_encoder low_low_pot" type="text" value="100" />
						<input class="rotary_encoder pan_pot" type="text" value="100" />-->
						<button class="mute_switch btn btn-warning btn-xs">M</button>
						<button class="solo_switch btn btn-primary btn-xs">S</button>
						<br />
						<button class="record_switch btn btn-danger btn-xs">R</button>
						<button class="lock_switch btn btn-info btn-xs">L</button>
					</div>
					<div class="mixer_channel">88</div>
					<span class="label label-default">{{channel}}</span><br />
					<p class="label label-primary">{{name}}</p>
				</li>
				{{/items}}
			</ul>
			<div class="well">
				<div class="btn-toolbar" role="toolbar">
					<div class="btn-group">
						<button type="button" class="btn btn-default"><i class="icon-step-backward"></i></button>
		  				<button type="button" class="btn btn-default"><i class="icon-backward"></i></button>
		  				<button type="button" class="btn btn-default"><i class="icon-forward"></i></button>
		  				<button type="button" class="btn btn-default"><i class="icon-step-forward"></i></button>
					</div>
					<div class="btn-group">
						<button type="button" class="btn btn-danger"><i class="icon-circle"></i></button>
		  				<button type="button" class="btn btn-default"><i class="icon-stop"></i></button>
		  				<button type="button" class="btn btn-success"><i class="icon-play"></i></button>
					</div>
					<div class="btn-group">
						<button type="button" class="btn btn-warning"><i class="icon-eject"></i></button>
					</div>
				</div>
			</div>
		</div>
		<div class="modal-footer">
			<button type="button" class="btn btn-default" data-dismiss="modal" aria-hidden="true">{{trans.close-verb}}</button>
		</div>
	</div>
</div>