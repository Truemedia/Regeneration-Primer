<!-- Map selector -->
<div class="page-header">
   <h1 style="text-align: center;">{{trans.choose_map_phrs}}</h1>
</div>
<div class="container">
  <div class="row">
    {{#maps}}
    <div class="col-sm-3">
      <div class="hero-widget well well-sm">
        <div style="height: 200px;" id="{{identifier}}"></div>
        <div class="text">
          <div class="caption">
            <h2>{{name}}</h2>
            <p style="height: 64px;">{{desc}}</p>
            <p>{{location.address}}</p>
            <p>{{location.town}}</p>
          </div>
        </div>
        <div class="progress" style="background: #ddd">
          <div class="bar bar-success" style="width: 100%;"></div>
        </div>
        <div class="row-fluid">
          <div class="pull-left"><b>Truemedia</b><br><small>{{trans.created_by_verb}}</small></div>
          <div class="pull-right"><b>N/A</b><br><small>{{trans.likes_verb}}</small></div>
        </div>
        <br class="clear" />
        <div class="options" style="text-align: center; padding-top: 38px;">
          <p>
            <button class="btn btn-default btn-lg map_select{{disabled}}" value="{{identifier}}">
              <span>{{trans.play_map_phrs}}</span>
            </button>
          </p>
        </div>
      </div>
    </div>
    {{/maps}}
  </div>
</div>