<!-- Map selector -->
<div class="page-header">
   <h1 style="text-align: center;">Choose a map</h1>
</div>
<div class="container">
  <div class="row">
    {{#maps}}
    <div class="col-sm-3">
      <div class="hero-widget well well-sm">
        <img alt="300x200" style="width: 100%" src="http://placehold.it/200x150">
        <div class="text">
          <div class="caption">
            <h2>{{name}}</h2>
            <p>{{desc}}</p>
            <p>{{location}}</p>
          </div>
        </div>
        <div class="progress" style="background: #ddd">
          <div class="bar bar-success" style="width: 100%;"></div>
        </div>
        <div class="row-fluid">
          <div class="span6"><b>Truemedia</b><br><small>CREATED BY</small></div>
          <div class="span6"><b>N/A</b><br><small>LIKES</small></div>
        </div>
        <hr />
        <div class="options">
          <button class="btn btn-default btn-lg map_select{{disabled}}" value="{{identifier}}">
            <span>Play Map</span>
          </button>
        </div>
      </div>
    </div>
    {{/maps}}
  </div>
</div>