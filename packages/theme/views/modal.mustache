<!-- Theme -->
<div class="modal-dialog">
	<div class="modal-content">
		<div class="modal-header">
			<button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
			<h4 id="theme-system-name">Themes</h4>
		</div>
		<div class="modal-body" style="max-height: 600px;">
			<section id="tables">
				<table id="swatch-menu" class="table table-bordered table-striped table-hover" style="margin-bottom: 10px;">
					<thead>
						<tr>
							<th>Theme</th>
							<th>Version</th>
							<th>Description</th>
							<th>Actions</th>
						</tr>
					</thead>
					<tbody>
						{{#themes}}
						<tr>
							<td>{{name}}</td>
							<td>{{version}}</td>
							<td>{{description}}</td>
							<td><a class="bootswatch-theme" href="{{cssMin}}" onclick="return false;"><img src="{{thumbnail}}" alt="{{description}}" style="display: none;" />Apply</a></td>
						</tr>
						{{/themes}}
					</tbody>
				</table>
			</section>
		</div>
		<div class="modal-footer">
			<button type="button" class="btn btn-default" data-dismiss="modal" aria-hidden="true">Close</button>
		</div>
	</div>
</div>