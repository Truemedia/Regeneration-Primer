<div class="row-fluid">
	<h3>{{#lang}}{{powered-by_phrs}}{{/lang}}</h3>
</div>

<footer class="row-fluid">

	<div class="span3">
    	<h4 class="line3 center standart-h4title"><span>{{#lang}}{{packages_noun}}{{/lang}}</span></h4>
        <div class="bs-example">
        	<ul style="max-width: 300px;" class="nav nav-pills nav-stacked">
        		{{#poweredby}}
            	<li class="dropdown">
                  <a href="{{website}}" data-toggle="dropdown" class="dropdown-toggle">
                    {{coreDependency}} + <span class="caret"></span>
                  </a>
                  <ul class="dropdown-menu">
                  	{{#dependencyPlugins}}
                    <li><a href="{{website}}">{{name}}</a></li>
                    {{/dependencyPlugins}}
                  </ul>
            	</li>
            	{{/poweredby}}
            </ul>
        </div>
    </div>
    
    {{#lang}}
    <div class="span3">
    	<h4 class="line3 center standart-h4title"><span>{{services_noun}}</span></h4>
    	<div class="bs-example">
        	<ul style="max-width: 300px;" class="nav nav-pills nav-stacked">
            	<li><a href="https://github.com/">Github</a></li>
                <li><a href="http://cdnjs.com/">CDNjs</a></li>
                <li class="dropdown">
                  <a href="http://getbootstrap.com/2.3.2/" data-toggle="dropdown" class="dropdown-toggle">
                    Bootstrap sites <span class="caret"></span>
                  </a>
                  <ul class="dropdown-menu">
                    <li><a href="http://bootswatch.com/">Bootswatch</a></li>
                    <li><a href="http://bootsnipp.com/">Bootsnipp</a></li>
                    <li><a href="http://bootstraptor.com">Bootstraptor</a></li>
                  </ul>
            	</li>
            </ul>
        </div>
    </div> 
	  
	<div class="span3">
    	<h4 class="line3 center standart-h4title"><span>{{contributors_noun}}</span></h4>
        <div class="bs-example">
        	<ul style="max-width: 300px;" class="nav nav-pills nav-stacked">
            	<li class="dropdown">
                  <a href="#" data-toggle="dropdown" class="dropdown-toggle">{{creator_name}} <span class="caret"></span></a>
                  <ul class="dropdown-menu">
                    <li><a href="https://github.com/Truemedia/Regeneration-Primer">GitHub</a></li>
                    <li><a href="http://www.youtube.com/user/MCOMediaCityOnline">YouTube</a></li>
                    <li class="divider"></li>
                    <li><a href="https://github.com/Truemedia">{{other-projects_sntc}}</a></li>
                  </ul>
            	</li>
            </ul>
        </div>
    </div>

    <div class="span3">
    	<h4 class="line3 center standart-h4title"><span>{{game-engine_name}}</span></h4>
			<p>
				{{original-credits_sntc}} <br/>
				<a href="http://github.com/Truemedia">{{creator_name}} (Truemedia)</a>
			</p>
			<p>
				{{sfx-credits_sntc}} <br />
				<a href="http://freesound.org">FreeSound</a>
			</p>
    </div>
  
	<div class="span12">
		<hr>
		<div class="row-fluid">
        	<div class="span6">
        		<p>&copy; {{copyright-notice_sntc}} 2013</p>
        	</div>
			<div>
				<p class="text-right">
					<span class="label label-info">{{language_noun}}</span>&nbsp;
					<span></span>&nbsp;
					<select><option value="lang">{{current-language_var}}</option></select>
					<select class="input-medium bfh-languages" data-language="en_US" data-available="en_US,fr_CA,es_MX"></select>
					<a href="https://github.com/Truemedia/Regeneration-Primer" class="btn btn-danger">{{volunteer_sntc}}</a>
				</p>
			</div>
		</div>
	</div>
	{{/lang}}
	  
</footer>