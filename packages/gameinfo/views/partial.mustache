<!-- Game Info -->
<div class="modal-body" style="height: 1200px;">
	<div class="span5">

      <h3 id="tabs">{{items.gamename}} - {{items.gamedescription}}</h3>
      <ul class="nav nav-tabs">
      	<li class=""><a data-toggle="tab" href="#feed_partial">News</a></li>
        <li class=""><a data-toggle="tab" href="#changelog">Updates</a></li>
		<li class=""><a data-toggle="tab" href="#videos">Videos</a></li>
		<li class=""><a data-toggle="tab" href="#screenshots">Screenshots</a></li>
      </ul>
      <div class="tabbable">
	  	<div class="tab-content">
	        <div id="feed_partial" class="tab-pane active"></div>
	        <div id="changelog" class="tab-pane">
	        	<p>Change log goes here</p>
	        </div>
	        <div id="videos" class="tab-pane">
	        	<p>Videos go here</p>
	        </div>
	        <div id="screenshots" class="tab-pane">
	        	<p>Screenshots go here</p>
	        </div>
	    	</div>
    	</div> <!-- /tabbable -->  
    </div>
</div>
<div class="modal-footer">
	<button class="btn" data-dismiss="modal" aria-hidden="true">Close</button>
</div>